function rangeOfNumbers(startNum, endNum) {
  const array = []
  if(startNum === endNum){
    array.push(endNum);
    return array;
  }else{
    const countArray = rangeOfNumbers(startNum + 1, endNum)
    countArray.unshift(startNum); 
    return countArray; 
  }

};


